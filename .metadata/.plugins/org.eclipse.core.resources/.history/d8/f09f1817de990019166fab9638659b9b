#include "stm32f30x.h"
#include "stm32f3xx_nucleo.h"
#include "tasks.h"

TaskHandle_t xTaskHandle1=NULL;
TaskHandle_t xTaskHandle2=NULL;
int main(void)
{
	RCC_DeInit();
	SystemCoreClockUpdate();

	//Create Task
	xTaskCreate(vTask1_handler, "Task1",configMINIMAL_STACK_SIZE, NULL, 2, &xTaskHandle1);
	for(;;);
}
